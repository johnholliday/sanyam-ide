{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "lib/**",
        "*.tsbuildinfo"
      ],
      "cache": true
    },
    "build:prod": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "lib/**",
        "*.tsbuildinfo"
      ],
      "cache": true
    },
    "sanyam-electron#build": {
      "dependsOn": [
        "^build",
        "sanyam-browser#build"
      ],
      "outputs": [
        "lib/**",
        "*.tsbuildinfo"
      ],
      "cache": true
    },
    "sanyam-electron#build:prod": {
      "dependsOn": [
        "^build",
        "sanyam-browser#build:prod"
      ],
      "outputs": [
        "lib/**",
        "*.tsbuildinfo"
      ],
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "lint:fix": {
      "cache": false
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "cache": true
    },
    "clean": {
      "cache": false
    },
    "watch": {
      "persistent": true,
      "cache": false
    }
  }
}