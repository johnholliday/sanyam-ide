  "scripts": {
    "clean": "theia clean && rimraf node_modules",
    "clean:dist": "rimraf dist",
    "generate:grammars": "node ../../packages/grammar-scanner/lib/generate-app-grammars.js",
    "build": "pnpm generate:grammars && npx theia rebuild:electron --cacheRoot ../.. && npx theia build --app-target=\"electron\" --mode development",
    "build:prod": "pnpm generate:grammars && npx theia rebuild:electron --cacheRoot ../.. && npx theia build --app-target=\"electron\"",
    "rebuild": "npx theia rebuild:electron --cacheRoot ../..",
    "watch": "concurrently -n compile,build \"theiaext watch --preserveWatchOutput\" \"theia build --watch --mode development\"",
    "start": "electron scripts/theia-electron-main.js --plugins=local-dir:../../plugins",
    "start:debug": "pnpm start --log-level=debug",
    "package": "pnpm clean:dist && npx theia rebuild:electron --cacheRoot ../.. && electron-builder -c.mac.identity=null --publish never",
    "package:prod": "pnpm deploy",
    "deploy": "pnpm clean:dist && npx theia rebuild:electron --cacheRoot ../.. && electron-builder -c.mac.identity=null --publish always",
    "package:preview": "pnpm clean:dist && npx theia rebuild:electron --cacheRoot ../.. && electron-builder -c.mac.identity=null --dir",
    "update:checksum": "ts-node scripts/update-checksum.ts",
    "update:blockmap": "ts-node scripts/update-blockmap.ts",
    "update:theia": "ts-node ../../scripts/update-theia-version.ts",
    "sign:directory": "ts-node scripts/sign-directory.ts",
    "test": "mocha --timeout 60000 \"./test/*.spec.js\"",
    "lint": "eslint --ext js,jsx,ts,tsx scripts && eslint --ext js,jsx,ts,tsx test",
    "lint:fix": "eslint --ext js,jsx,ts,tsx scripts --fix && eslint --ext js,jsx,ts,tsx test -fix"
  }
}