  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#8b949e"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "name": "Entity",
      "scope": [
        "entity.name"
      ],
      "settings": {
        "foreground": "#d2a8ff"
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution"
      ],
      "settings": {
        "foreground": "#ffa657"
      }
    },
    {
      "name": "Entity Function",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#d2a8ff"
      }
    },
    {
      "name": "Entity Tag",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#f85149"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.alignof",
        "keyword.operator.typeid",
        "keyword.operator.alignas",
        "keyword.operator.instanceof",
        "keyword.operator.logical.python",
        "keyword.operator.wordlike"
      ],
      "settings": {
        "foreground": "#ff7b72"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#ff7b72"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#ff7b72"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#a5d6ff"
      }
    },
    {
      "name": "String Regexp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#ffa657"
      }
    },
    {
      "name": "Support Variable",
      "scope": "support.variable",
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#e6edf3"
      }
    },
    {
      "name": "Variable Language",
      "scope": "variable.language",
      "settings": {
        "foreground": "#ff7b72"
      }
    },
    {
      "name": "Variable Parameter",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#e6edf3"
      }
    },
    {
      "name": "Attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation"
      ],
      "settings": {
        "foreground": "#e6edf3"
      }
    },
    {
      "name": "Punctuation Accessor",
      "scope": [
        "punctuation.accessor",
        "punctuation.separator"
      ],
      "settings": {
        "foreground": "#ff7b72"
      }
    },
    {
      "name": "Meta Brace",
      "scope": [
        "meta.brace"
      ],
      "settings": {
        "foreground": "#e6edf3"
      }
    },
    {
      "name": "Markup Heading",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#79c0ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
