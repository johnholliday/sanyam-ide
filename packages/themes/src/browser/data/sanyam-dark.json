      }
    },
    {
      "name": "Markup Fenced Code",
      "scope": [
        "markup.fenced_code.block",
        "markup.raw.block"
      ],
      "settings": {
        "foreground": "#e6edf3"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "name": "Markup List",
      "scope": [
        "markup.list",
        "punctuation.definition.list"
