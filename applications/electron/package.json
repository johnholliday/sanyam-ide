  "main": "scripts/theia-electron-main.js",
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "John F. Holliday",
    "email": "sanyam@johnholliday.net"
  },
  "homepage": "https://github.com/johnholliday/sanyam-ide#readme",
  "bugs": {
    "url": "https://github.com/johnholliday/sanyam-ide/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnholliday/sanyam-ide.git"
  },
  "engines": {
    "pnpm": ">=9",
    "node": ">=20"
  },
  "theia": {
    "target": "electron",
    "frontend": {
      "config": {
        "applicationName": "Enterprise Content Modeling Language IDE",
        "applicationData": {
          "name": "Enterprise Content Modeling Language IDE",
          "description": "Development environment for Enterprise Content Modeling Language domain-specific language",
          "logo": "resources/sanyam-banner.svg",
          "grammarId": "ecml",
          "grammarLogo": "data:image/svg+xml;base64,",
          "tagline": "Build and edit Enterprise Content Modeling Language models with ease",
          "text": [
            "Enterprise Content Modeling Language IDE provides a complete development environment for creating and working with Enterprise Content Modeling Language files.",
            "Features include syntax highlighting, validation, code completion, and diagram generation for your Enterprise Content Modeling Language models."
          ],
          "links": [
            {
              "label": "Documentation",
              "url": "https://github.com/johnholliday/sanyam-ide#readme",
              "icon": "book"
            },
            {
              "label": "GitHub",
              "url": "https://github.com/johnholliday/sanyam-ide",
              "icon": "github"
            },
            {
              "label": "Report Issue",
              "url": "https://github.com/johnholliday/sanyam-ide/issues",
              "icon": "bug"
            }
          ]
        },
        "reloadOnReconnect": true,
        "preferences": {
          "toolbar.showToolbar": true,
          "security.workspace.trust.enabled": false
        },
        "electron": {
          "showWindowEarly": false,
          "splashScreenOptions": {
            "content": "resources/sanyam-splash.svg",
            "height": 235,
            "width": 500
          }
        }
      }
    },
    "backend": {
      "config": {
        "frontendConnectionTimeout": -1,
        "startupTimeout": -1,
        "resolveSystemPlugins": false,
        "configurationFolder": ".sanyam-ide"
      }
    },
    "generator": {
      "config": {
        "preloadTemplate": "./resources/preload.html"
      }
    }
  },
