{
  "name": "sanyam",
  "private": true,
  "version": "1.67.100",
  "license": "MIT",
  "author": "John Holliday <john@johnholliday.net>",
  "homepage": "https://github.com/johnholliday/sanyam-ide#readme",
  "bugs": {
    "url": "https://github.com/johnholliday/sanyam-ide/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnholliday/sanyam-ide.git"
  },
  "packageManager": "pnpm@10.29.2",
  "engines": {
    "pnpm": ">=9",
    "node": ">=20"
  },
  "devDependencies": {
    "@eclipse-dash/nodejs-wrapper": "^0.0.1",
    "@theia/cli": "1.67.0",
    "@vitest/coverage-v8": "^2.0.0",
    "@types/yargs": "17.0.7",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-plugin-deprecation": "1.2.1",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-no-unsanitized": "^3.1.5",
    "eslint-plugin-react": "^7.23.2",
    "node-gyp": "^11.4.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.0.0",
    "turbo": "^2.8.3",
    "type-fest": "^0.20.2",
    "vitest": "^2.0.0",
    "yargs": "17.2.1"
  },
  "scripts": {
    "browser": "pnpm --dir applications/browser",
    "build": "pnpm build:browser",
    "build:all": "turbo run build --filter=\"sanyam*\" --filter=\"@sanyam/*\" --filter=\"@sanyam-ide/*\" --filter=\"@sanyam-grammar/*\"",
    "build:browser": "turbo run build --filter=sanyam-browser && pnpm deploy:language-server",
    "build:browser:prod": "turbo run build:prod --filter=sanyam-browser && pnpm deploy:language-server",
    "build:electron": "turbo run build --filter=sanyam-electron && pnpm deploy:language-server",
    "build:electron:prod": "turbo run build:prod --filter=sanyam-electron && pnpm deploy:language-server",
    "build:electron:win": "bash scripts/build-electron-win.sh",
    "build:extensions": "turbo run build --filter=\"@sanyam-ide/*\"",
    "build:grammars": "turbo run build --filter=\"@sanyam-grammar/*\"",
    "build:language-server": "pnpm --filter sanyam-language-server run build:extension",
    "build:linux": "SKIP_WIN_BUILD=1 pnpm build",
    "build:prod": "turbo run build:prod --filter=\"sanyam*\" --filter=\"@sanyam/*\" --filter=\"@sanyam-ide/*\" --filter=\"@sanyam-grammar/*\"",
    "clean": "pnpm -r run clean && rimraf node_modules .turbo",
    "deploy:language-server": "pnpm build:language-server && pnpm --filter sanyam-language-server run package:vsix && rm -rf plugins/sanyam.sanyam-language-server && mkdir -p plugins/sanyam.sanyam-language-server && unzip -o packages/language-server/sanyam-language-server-*.vsix -d plugins/sanyam.sanyam-language-server/",
    "debug:browser": "bash scripts/launch-browser-debug.sh",
    "docs:build": "bash scripts/docs.sh build",
    "docs:clean": "bash scripts/docs.sh clean",
    "docs:start": "bash scripts/docs.sh start",
    "download:plugins": "theia download:plugins --rate-limit=15 --parallel=false && pnpm permissions:writeable && pnpm deploy:language-server",
    "electron": "pnpm --dir applications/electron",
    "license:check": "npx dash-licenses-wrapper  --configFile=./configs/license-check-config.json",
    "license:check:review": "npx dash-licenses-wrapper  --configFile=./configs/license-check-config.json --review",
    "lint": "eslint --ext js,jsx,ts,tsx scripts && turbo run lint",
    "lint:fix": "eslint --ext js,jsx,ts,tsx scripts --fix && turbo run lint:fix",
    "package:applications": "pnpm --workspace-concurrency=1 --filter \"sanyam*\" --filter \"!@sanyam/*\" --filter \"!@sanyam-ide/*\" --filter \"!@sanyam-grammar/*\" run package",
    "package:applications:preview": "pnpm --workspace-concurrency=1 --filter \"sanyam*\" --filter \"!@sanyam/*\" --filter \"!@sanyam-ide/*\" --filter \"!@sanyam-grammar/*\" run package:preview",
    "package:applications:prod": "pnpm --workspace-concurrency=1 --filter \"sanyam*\" --filter \"!@sanyam/*\" --filter \"!@sanyam-ide/*\" --filter \"!@sanyam-grammar/*\" run package:prod",
    "permissions:writeable": "ts-node scripts/make-files-writeable.ts plugins",
    "postbuild": "pnpm deploy:language-server && bash scripts/postbuild-electron-win.sh",
    "postinstall": "theia-patch && node scripts/ensure-ripgrep.js",
    "prebuild": "bash scripts/swap-native-modules.sh linux && pnpm rebuild drivelist",
    "rebuild:electron:win": "bash scripts/rebuild-electron-win.sh",
    "setup:electron:win": "bash scripts/setup-electron-win.sh",
    "start:browser": "pnpm theia:browser start",
    "start:electron": "pnpm theia:electron start",
    "start:electron:win": "bash scripts/dev-electron-win.sh",
    "structurizr": "bash scripts/structurizr.sh",
    "structurizr:rebuild": "bash ./scripts/structurizr.sh --rebuild",
    "test": "turbo run test",
    "test:unit": "vitest run --project unit",
    "test:integration": "vitest run --project integration",
    "test:db": "vitest run --project database",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --project unit",
    "test:e2e": "pnpm --filter @sanyam/e2e test",
    "test:e2e:headed": "pnpm --filter @sanyam/e2e test:headed",
    "test:e2e:ui": "pnpm --filter @sanyam/e2e test:ui",
    "theia:browser": "pnpm --dir applications/browser",
    "theia:bundle": "pnpm theia:electron bundle && pnpm theia:browser bundle",
    "theia:electron": "pnpm --dir applications/electron",
    "update:theia": "ts-node scripts/update-theia-version.ts",
    "update:theia:children": "pnpm -r run update:theia -- ",
    "watch": "turbo run watch --concurrency=20",
    "watch:ide": "turbo run watch --filter=\"@sanyam-ide/*\"",
    "go": "pnpm build:browser && pnpm start:browser",
    "go:force": "rm -rf node_modules/.cache/turbo && pnpm go"
  },
  "theiaPluginsDir": "plugins",
  "theiaPlugins": {
    "eclipse-theia.builtin-extension-pack": "https://open-vsx.org/api/eclipse-theia/builtin-extension-pack/1.95.3/file/eclipse-theia.builtin-extension-pack-1.95.3.vsix"
  },
  "theiaPluginsExcludeIds": [
    "ms-vscode.js-debug-companion",
    "VisualStudioExptTeam.vscodeintellicode",
    "vscode.extension-editing",
    "vscode.github",
    "vscode.github-authentication",
    "vscode.microsoft-authentication"
  ],
  "workspaces": [
    "packages/*",
    "packages/theia-extensions/*",
    "packages/grammar-definitions/*",
    "packages/grammar-definitions/*/docs",
    "applications/*",
    "tests/*"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@theia/ffmpeg",
      "@vscode/ripgrep",
      "@vscode/windows-ca-certs",
      "better-sqlite3",
      "chromedriver",
      "cpu-features",
      "drivelist",
      "electron",
      "electron-chromedriver",
      "electron-winstaller",
      "esbuild",
      "keytar",
      "msgpackr-extract",
      "native-keymap",
      "node-pty",
      "protobufjs",
      "puppeteer",
      "ssh2"
    ],
    "supportedArchitectures": {
      "cpu": [
        "x64"
      ],
      "os": [
        "linux",
        "win32"
      ]
    },
    "overrides": {
      "@types/puppeteer": "^5.4.0",
      "@yarnpkg/parsers": "3.0.0-rc.44",
      "multer": "1.4.4-lts.1",
      "nan": "2.23.0",
      "cpu-features": "0.0.9",
      "perfect-scrollbar": "1.5.5",
      "node-abi": "4.14.0"
    }
  }
}