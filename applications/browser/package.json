    "@theia/cli": "1.67.0",
    "@theia/native-webpack-plugin": "1.67.0"
  },
  "scripts": {
    "clean": "theia clean && rimraf node_modules",
    "build": "npx theia rebuild:browser --cacheRoot ../.. && npx theia build --app-target=\"browser\" --mode development",
    "build:prod": "npx theia rebuild:browser --cacheRoot ../.. && npx theia build --app-target=\"browser\"",
    "rebuild": "npx theia rebuild:browser --cacheRoot ../..",
    "start": "theia start --port=3002 --plugins=local-dir:../../plugins",
    "watch": "concurrently --kill-others -n tsc,build -c red,yellow \"tsc -b -w --preserveWatchOutput\" \"pnpm watch:bundle\"",
    "update:theia": "ts-node ../../scripts/update-theia-version.ts"
  }
}