{
  "private": true,
  "name": "sanyam-browser",
  "description": "Sanyam IDE browser product",
  "productName": "Sanyam IDE",
  "version": "1.67.100",
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "John F. Holliday",
    "email": "sanyam@johnholliday.net"
  },
  "homepage": "https://github.com/johnholliday/sanyam-ide#readme",
  "bugs": {
    "url": "https://github.com/johnholliday/sanyam-ide/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnholliday/sanyam-ide.git"
  },
  "engines": {
    "pnpm": ">=9",
    "node": ">=20"
  },
  "theia": {
    "frontend": {
      "config": {
        "applicationName": "Sanyam IDE",
        "warnOnPotentiallyInsecureHostPattern": false,
        "preferences": {
          "toolbar.showToolbar": true,
          "files.enableTrash": false,
          "security.workspace.trust.enabled": false
        },
        "reloadOnReconnect": true
      }
    },
    "backend": {
      "config": {
        "warnOnPotentiallyInsecureHostPattern": false,
        "startupTimeout": -1,
        "resolveSystemPlugins": false,
        "configurationFolder": ".sanyam-ide",
        "frontendConnectionTimeout": 3000
      }
    },
    "generator": {
      "config": {
        "preloadTemplate": "./resources/preload.html"
      }
    }
  },
  "dependencies": {
    "@theia/ai-anthropic": "1.67.0",
